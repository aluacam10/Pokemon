package Pokemon;
public class BatallasPokemon {
    public static void main(String[] args) {
        PokemonTierra charmander = new PokemonTierra("Rhydon");
        Pokemon squirtle = new Pokemon("Noibat", "Aire", 10);

        // Rhydon ataca a Noibat
        Rhydon.atacar("Disparo lodo	", Noibat);

        // Squirtle ataca a Rhydon
        Noibat.atacar("Ataque aereo", Rhydon);

        // Imprimir los puntos de vida de los Pok√©mones
        System.out.println(Rhydon.getNombre() + ": HP " + Rhydon.getHP());
        System.out.println(Noibat.getNombre() + ": HP " + Noibat.getHP());
    }
